@model WebProject.Models.ViewModels.CreateRankingViewModel
@{
    ViewBag.Title = "Add Ranking";
}

<h2>Add Ranking</h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)
    @Html.DropDownListFor(model => model.Score, new SelectList(new int[] { 1, 2, 3, 4, 5 }), new { @class="form-control" })
    @Html.HiddenFor(model => model.UserEmail, new {@Value = Model.UserEmail })
    @Html.HiddenFor(model => model.PlaceId, new { @Value = ViewBag.PlaceId })
<button type = "submit" class="btn btn-default">Add</button>
}